{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "typeRoots" : ["./custom.d.ts"],
  },
  "include": ["src/**/*.ts", "src/custom.d.ts"],
  "exclude": ["node_modules", "dist"],

  
}